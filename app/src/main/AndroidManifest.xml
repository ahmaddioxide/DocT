<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.atinity.doct">

    <application
        android:allowBackup="true"
        android:icon="@drawable/doct_iconbig"
        android:label="@string/app_name"
        android:roundIcon="@drawable/doct_iconbig"
        android:supportsRtl="true"
        android:theme="@style/Theme.DocT"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".RegistrationOrSignupActivity"
            android:exported="false" />
        <activity
            android:name=".HomeChatActivity"
            android:exported="false" />
        <activity
            android:name=".ChatWelcomeActivity"
            android:exported="false" />
        <activity
            android:name=".PatientDoctorSelectionActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>